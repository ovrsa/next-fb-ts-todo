{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "es5",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "pages/firebase.tssx", "pages/todos.", "pages/createTodo.tsx", "pages/firebase.tsxxx"  ],
  "exclude": [
    "node_modules"
  ]
}
